#
# Tcl package index file
#
if {[package vsatisfies [package provide Tcl] 9.0]} {
    package ifneeded @PACKAGE_NAME@ @PACKAGE_VERSION@ [list apply {
	dir {
	    load [file join $dir @PKG_LIB_FILE9@]
	}
    } $dir]
} else {
    package ifneeded @PACKAGE_NAME@ @PACKAGE_VERSION@ [list apply {
	dir {
	    load [file join $dir @PKG_LIB_FILE8@]
	}
    } $dir]
}
