} -result null
#>>>
test extract-5.1 {extract a value from an array in an object} -body { #<<<
	json extract {
		{
			"foo": "bar",
			"baz":  ["str", 123, 123.4, true, false, null, {"inner": "obj"}]
		}
	} baz 2
} -result 123.4
#>>>
test extract-6.1 {extract of a toplevel value} -body { #<<<
	# Note - should not be normalized
	json extract {
		{
			"foo": "bar",
			"baz":  ["str", 123, 123.4, true, false, null, {"inner": "obj"}]
		}
	}
} -result {
		{
			"foo": "bar",
			"baz":  ["str", 123, 123.4, true, false, null, {"inner": "obj"}]
		}
	}
#>>>

