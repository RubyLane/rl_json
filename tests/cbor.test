# cbor-10.* cbor get - wellformed
proc test_get {num bytes res {tags {}}} {
	tailcall test cbor_get-$num $res -body "set res \[cbor tget tags \[binary decode hex [list $bytes]\]\]; list \$res \$tags" -result [list $res $tags] -cleanup {unset -nocomplain res tags}
}

